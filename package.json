    "expo-modules-core": "^3.0.26",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.4",
    "lighthouse": "^12.8.2",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.3.3"
  }
