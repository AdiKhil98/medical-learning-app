{
  "expo": {
    "name": "KP Med",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "medicallearningapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "shortName": "KP Med",
      "lang": "de",
      "themeColor": "#D4A574",
      "backgroundColor": "#F8FAFC",
      "meta": {
        "viewport": "width=device-width, initial-scale=1, viewport-fit=cover",
        "theme-color": "#D4A574",
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "default",
        "og:type": "website",
        "og:site_name": "KP Med",
        "og:title": "KP Med - Medizinische Prüfungsvorbereitung für Ärzte",
        "og:description": "Professionelle Vorbereitung für KP und FSP Prüfungen. Interaktive Simulationen für internationale Ärzte in Deutschland.",
        "og:locale": "de_DE",
        "twitter:card": "summary_large_image",
        "twitter:title": "KP Med - Medizinische Prüfungsvorbereitung",
        "twitter:description": "Professionelle Vorbereitung für KP und FSP Prüfungen für internationale Ärzte."
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "sounds": ["./assets/sounds/notification.wav"]
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
